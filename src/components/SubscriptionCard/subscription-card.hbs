<div id="sub_{{id}}" class="sub sub__sub">
  <span class="sub__name">{{subName}}</span>
  <span class="sub__lvl">Уровень {{lvl}}</span>
  {{{subImg}}}
  <div class="price">
    <span class="price__count">&#8381;{{price}}</span>
    <span class="price__period">{{period}}</span>
  </div>
  {{{button}}}
  <div lang="ru" id="motivation_{{id}}" class="sub__motivation">{{motivation}}</div>
</div>