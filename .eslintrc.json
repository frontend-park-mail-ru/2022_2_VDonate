{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "globals": {
        "Handlebars": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "dist/*",
        "webpack.config.js"
    ],
    "rules": {
        "space-in-parens": "error"
    }
}