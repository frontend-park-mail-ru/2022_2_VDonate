{
  "name": "vdonate",
  "version": "1.0.0",
  "description": "Фронтенд для итогового проекта 2 семестра проекта \"VK образование\" команды ВДонате.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx stylus -c -o public src/style.styl && npx handlebars src/template/ -f public/all.precompiled.js",
    "stylus": "npx stylus -c -w src -o public",
    "lint": "npx eslint src/",
    "lint:fix": "npx eslint src/ --fix",
    "pre-commit": "npx eslint src/ --max-warnings=0 --fix",
    "prepare": "husky install",
    "build:dev": "npx webpack --mode development",
    "build:prod": "npx webpack --mode production",
    "stats": "npx webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontend-park-mail-ru/2022_2_VDonate.git"
  },
  "author": "VDonate Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frontend-park-mail-ru/2022_2_VDonate/issues"
  },
  "homepage": "https://github.com/frontend-park-mail-ru/2022_2_VDonate#readme",
  "dependencies": {
    "handlebars": "^4.7.7",
    "stylus": "^0.59.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-config-google": "^0.14.0",
    "handlebars-loader": "^1.7.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "style-loader": "^3.3.1",
    "stylus-loader": "^7.1.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0"
  }
}
